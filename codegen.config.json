{
  "feature": {
    "model": {
      "path": "./demo/models",
      "filename": "{name}.model.js",
      "template": "mongoose"
    },
    "controller": {
      "path": "./demo/controllers",
      "filename": "{name}.controller.js",
      "template": "es5"
    },
    "route": {
      "path": "./demo/routes",
      "filename": "{name}.route.js",
      "template": "express"
    },
    "page": {
      "path": "./demo/pages/{Name}",
      "filename": "index.jsx",
      "template": "reactjs"
    }
  },
  "hook": {
    "": {
      "path": "./demo/hooks",
      "filename": "use{Name}.jsx",
      "template": "reactjs-hook"
    }
  }
}
